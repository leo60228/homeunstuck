{
    "manifest_version": 2,
    "name": "HomeUnstuck",
    "version": "0.3",
    "permissions": [
        "storage",
        "webNavigation"
      ],
    "options_page": "options.html",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
      },
    "content_scripts": [
        {
            "matches": [
                "https://mspfa.com/?s=2302*",
                "https://www.deconreconstruction.com/*"
            ],
            "js": [
                "vastError.js",
                "homestuck.js",
                "textChangers.js",
                "content.js"
            ]
        }
    ]
}